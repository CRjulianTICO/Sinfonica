@model Sinfonica.Web.Areas.Admin.Data.Entities.ProfesorCurso

@{
    ViewData["Title"] = "Details";
}

<section class="content-header">
        <h1>
            Cursos - Profesores
            <small>Detalles</small>
        </h1>
        <ol class="breadcrumb">
            <li><a asp-action="Index"><i class="fa fa-arrow-left"></i> Regresar</a></li>
        </ol>
    </section>
<section class="content">
    <div class="row">
        <div class="col-md-6">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Está seguro de que desea eliminar esta entrada?</h3>
                </div>
                <div class="box-body">
                     <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Profesors)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Profesors.NombreCompleto)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Cursos)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Cursos.NombreCurso)
        </dd>
    </dl>
                </div>
            </div>
        </div>
    </div>
      <!-- /.row -->
    </section>

