                    <div class="footer_content">
                        <div class="row">

                            <!-- Footer Column - About -->
                            <div class="col-lg-6 footer_col">

                                <!-- Logo -->
                                <div class="logo_container">
                                    <div class="logo">
                                        <img src="images/cor66.png" alt="">
                                        <span>INM</span>
                                    </div>
                                </div>

