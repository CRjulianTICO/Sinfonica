    <div class="super_container">

        <!-- Header -->




        <header class="header d-flex flex-row">








            <div class="header_content d-flex flex-row align-items-center">
                <!-- Logo -->
                <div class="logo_container">
                    <div class="logo">
                        <!--<img src="~/images/logo3.png" alt="">-->
                        <span>INM</span>
                    </div>
                </div>

                <!-- Main Navigation -->
                <nav class="main_nav_container">
                    <div class="main_nav">
                        <ul class="main_nav_list">
                            <li class="main_nav_item"><a asp-area="User" asp-controller="Home" asp-action="Index">Inicio</a></li>
                            <li class="main_nav_item"><a href="#">Sobre Nosotros</a></li>
                            <li class="main_nav_item"><a asp-area="User" asp-controller="Cursoes" asp-action="Index">Cursos</a></li>
                            <li class="main_nav_item"><a href="#">elements</a></li>
                            <li class="main_nav_item"><a asp-area="User" asp-controller="Eventos" asp-action="Index">Eventos</a></li>
                            <li class="main_nav_item"><a href="#">contact</a></li>
                        </ul>
                    </div>
                </nav>
            </div>
            <div class="header_side d-flex flex-row justify-content-center align-items-center">
                <img src="~/images/phone-call.svg" alt="">
                <span>2240 0333</span>
            </div>

            <!-- Hamburger -->
            <div class="hamburger_container">
                <i class="fas fa-bars trans_200"></i>
            </div>

        </header>

