@model Sinfonica.Web.Areas.Admin.Models.AcontecersViewModel

@{
    ViewData["Title"] = "Create";
}










<link rel="stylesheet" type="text/css" href="~/css/contact_styles.css">
<link rel="stylesheet" type="text/css" href="~/css/contact_responsive.css">




<!-- Home -->

<div class="home">
    <div class="home_background_container prlx_parent">
        <div class="home_background prlx" style="background-image:url(/images/contact_background.jpg)"></div>
    </div>
    <div class="home_content">
        <h1>Acontecer</h1>
    </div>
</div>






<!-- Contact -->

<div class="contact">
    <div class="container">
        <div class="row">
            <div class="col-lg-8">

                <!-- Contact Form -->
                <div class="contact_form">
                    <div class="contact_title">Escribenos</div>

                    <div class="contact_form_container">


                        <form asp-action="Create" enctype="multipart/form-data">
                            <input id="contact_form_name" class="input_field contact_form_name" type="text" placeholder="Numero de Carnet" required="required" data-error="Carnet es requerido." asp-for="Estudiantes.Carnet" pattern="\d*">
                            <input id="contact_form_name" class="input_field contact_form_name" type="text" placeholder="Titulo" required="required" data-error="Name is required." asp-for="Titulo">
                            <input id="contact_form_email" class="input_field contact_form_email" type="text" placeholder="Encabezado" required="required" data-error="Valid email is required." asp-for="Encabezado">
                            <input id="contact_form_email" class="input_field contact_form_email" type="text" placeholder="Lugar" required="required" data-error="Valid email is required." asp-for="Lugar">
                            <input asp-for="Fecha" class="form-control input_field" />
                            <textarea id="contact_form_message" class="text_field contact_form_message" name="Descripcion" placeholder="Descripcion" required="required" data-error="Please, write us a message." asp-for="Descripcion"></textarea>
                            <textarea id="contact_form_message" class="text_field contact_form_message" name="Informacion" placeholder="Informacion" required="required" data-error="Please, write us a message." asp-for="Informacion"></textarea>
                            <input asp-for="ImageFile" class="form-control input_field" type="file" />
                            <input id="contact_send_btn" type="submit" class="contact_send_btn trans_200" value="Publicar" />
                        </form>


                    </div>
                </div>

            </div>


        </div>



    </div>

    <script src="~/js/jquery-3.2.1.min.js"></script>

    <script src="~/css/bootstrap4/popper.js"></script>
    <script src="~/css/bootstrap4/bootstrap.min.js"></script>
    <script src="~/plugins/greensock/TweenMax.min.js"></script>
    <script src="~/plugins/greensock/TimelineMax.min.js"></script>
    <script src="~/plugins/scrollmagic/ScrollMagic.min.js"></script>
    <script src="~/plugins/greensock/animation.gsap.min.js"></script>
    <script src="~/plugins/greensock/ScrollToPlugin.min.js"></script>
    <script src="~/plugins/OwlCarousel2-2.2.1/owl.carousel.js"></script>
    <script src="~/plugins/scrollTo/jquery.scrollTo.min.js"></script>
    <script src="~/plugins/easing/easing.js"></script>
    <script src="~/js/custom.js"></script>
    <script src="~/js/videobg.js"></script>

