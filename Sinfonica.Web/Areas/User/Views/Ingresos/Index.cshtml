@model IEnumerable<Sinfonica.Web.Areas.Admin.Data.Entities.Ingreso>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AntesAplicar)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RequisitosNacionales)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RequisitosExtranjeros)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Formulario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pruebas)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AntesAplicar)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RequisitosNacionales)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RequisitosExtranjeros)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Formulario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pruebas.Titulo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
